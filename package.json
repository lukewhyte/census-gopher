{
  "name": "census-gopher",
  "version": "0.1.0",
  "description": "Command line tool for grabbing micro census data at macro level",
  "preferGlobal": true,
  "bin": {
    "gopher": "src/index.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "flow": "flow",
    "test": "mocha -w --require babel-polyfill --compilers js:babel-register",
    "lint": "jshint .",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sa-express-news/census-gopher.git"
  },
  "keywords": [
    "census",
    "gopher",
    "geographic",
    "variables",
    "excel"
  ],
  "author": "Luke Whyte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sa-express-news/census-gopher/issues"
  },
  "homepage": "https://github.com/sa-express-news/census-gopher#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.0.0",
    "flow-bin": "^0.48.0",
    "mocha": "^3.4.2",
    "precommit-hook": "^3.0.0"
  },
  "dependencies": {
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ]
}